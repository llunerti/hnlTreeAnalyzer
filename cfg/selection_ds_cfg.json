{
	"categories":[
		{"printout":"inclusive category (no cuts)",
		 "label"   :"inclusive",
		 "cut"     :"C_mu1_Phi_charge!=0"}
	],
	"selection_eff_scan": [
		{"name"    : "C_Ds_lxy",
		 "low_edge": -1.0,
		 "up_edge" : 30.0,
		 "logic"   : ">"
		},
		{"name"    : "C_Ds_pt",
		 "low_edge": -1.0,
		 "up_edge" : 30.0,
		 "logic"   : ">"
		}
	],
	"new_variables" : [
		{"name"	:"C_Ds_lxy",
		 "definition":"sqrt(C_Ds_vertex_x*C_Ds_vertex_x + C_Ds_vertex_y*C_Ds_vertex_y)"
		},
		{"name"	:"C_mu_B_matched_MU8_IP3",
		 "definition":"get_mu_trigger_matching(HLT_mu8_ip3_matched,C_mu_trig_idx, C_mu_B_idx)"
		},
		{"name"	:"C_mu_B_matched_MU9_IP6",
		 "definition":"get_mu_trigger_matching(HLT_mu9_ip6_matched,C_mu_trig_idx, C_mu_B_idx)"
		},
		{"name"	:"C_mu1_Phi_matched_MU8_IP3",
		 "definition":"get_mu_trigger_matching(HLT_mu8_ip3_matched,C_mu_trig_idx, C_mu1_Phi_idx)"
		},
		{"name"	:"C_mu1_Phi_matched_MU9_IP6",
		 "definition":"get_mu_trigger_matching(HLT_mu9_ip6_matched,C_mu_trig_idx, C_mu1_Phi_idx)"
		},
		{"name"	:"C_mu2_Phi_matched_MU8_IP3",
		 "definition":"get_mu_trigger_matching(HLT_mu8_ip3_matched,C_mu_trig_idx, C_mu2_Phi_idx)"
		},
		{"name"	:"C_mu2_Phi_matched_MU9_IP6",
		 "definition":"get_mu_trigger_matching(HLT_mu9_ip6_matched,C_mu_trig_idx, C_mu2_Phi_idx)"
		}
	],
	"best_cand_var" :
		{"name"	:"best_cand_idx",
		 "definition":"get_best_cand_idx(C_Bs_pt)"
		},	
	"selection_cuts" :[
		{"cut":"C_mu_B_matched_MU8_IP3>0 || C_mu_B_matched_MU9_IP6>0 || C_mu1_Phi_matched_MU8_IP3>0 || C_mu1_Phi_matched_MU9_IP6>0 || C_mu2_Phi_matched_MU8_IP3>0 || C_mu2_Phi_matched_MU9_IP6>0",
		 "printout":"at least one mu matched MU9_IP6 || MU8_IP3"
		},
		{"cut":"(C_mu1_Phi_charge+C_mu2_Phi_charge)==0",
		 "printout":"opposite charge muons"
		},
		{"cut":"C_mu1_Phi_pt>3.0",
		 "printout":"leading muon from Phi pt>1.5"
		},
		{"cut":"C_mu1_Phi_isSoft>0",
		 "printout":"leading muon from Phi is soft"
		},
		{"cut":"C_mu2_Phi_pt>1.5",
		 "printout":"sub-leading muon from Phi pt>1.5"
		},
		{"cut":"C_mu2_Phi_isSoft>0",
		 "printout":"sub-leading muon from Phi is soft"
		}
	],
	"gen_matching_cuts" :[
		{"cut":"C_mu1_isHnlDaughter>0",
		 "printout":"muon matching HNL mu daughter"
		},
		{"cut":"C_pi_isHnlDaughter>0",
		 "printout":"pi matching HNL pi daughter"
		},
		{"cut":"C_mu_B_isHnlBrother>0",
		 "printout":"trigger muon is HNL brother"
		}
	]
}
